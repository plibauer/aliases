unalias '*'
set alias_dir     = ~/aliases/tcsh

source $alias_dir/aliases_local.txt

set revision      = 73
set DBID          = D1
set defArch       = x64
set defCompiler   = VS2008
set defBuild      = Debug
set defPatch      = ""
set SVN_MASTER    = $SVN_HOUSTON

source $alias_dir/aliases
source $alias_dir/aliases_common.txt

settitle
