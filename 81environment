unalias '*'
set alias_dir     = ~/aliases

source $alias_dir/aliases_local.txt

set revision      = 81
set DBID          = F1
set defArch       = x64
set defCompiler   = VS2013
set defBuild      = Debug
set trimdir       = $REPOS_ROOT":/$revision"
set dbDefault     = Test_"$revision"_SqlServer
set cygtrimdir    = "/$REPOS_ROOT/$revision"
set idolDb        = TRIM_"$DBID"
set trimcfgdir    = "$trimdir/trim/SharedTrimServerData/ServerData"
set currentEnv    = ~/aliases/"$revision"environment

source $alias_dir/aliases_common.txt
source $alias_dir/aliases

settitle
